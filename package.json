{
  "name": "vite-react-web-lib-starter",
  "description": "unknown",
  "author": "unknown<https://github.com/unknown>",
  "version": "0.0.0",
  "keywords": [
    "unknown"
  ],
  "type": "module",
  "homepage": "unknown",
  "sideEffects": [],
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "exports": {
    ".": {
      "browser": {
        "production": "./dist/index.umd.js"
      },
      "import": "./dist/index.es.js"
    }
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "lint-staged": {
    "*.(j|t)s?(x)": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "vite",
    "commit": "cz",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "initialize": "pnpm i && git init && husky install",
    "typecheck": "tsc --noEmit",
    "build": "tsc --noEmit && vite build && tsc --emitDeclarationOnly",
    "release": "tsc --noEmit && vite build && tsc --emitDeclarationOnly && release-it",
    "lint": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore"
  },
  "peerDependencies": {
    "react": "^16.8.0  || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0  || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@commitlint/cz-commitlint": "^17.0.3",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/node": "^18.0.3",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@vitejs/plugin-react": "^1.3.0",
    "c8": "^7.11.3",
    "commitizen": "^4.2.4",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "jsdom": "^21.1.0",
    "postcss-preset-env": "^7.7.2",
    "postcss-scss": "^4.0.4",
    "prettier": "2.7.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-test-renderer": "^18.2.0",
    "release-it": "^15.5.0",
    "sass": "^1.53.0",
    "typescript": "^4.6.3",
    "vite": "^4.1.4",
    "vite-plugin-eslint": "^1.6.1",
    "vitest": "^0.29.1"
  }
